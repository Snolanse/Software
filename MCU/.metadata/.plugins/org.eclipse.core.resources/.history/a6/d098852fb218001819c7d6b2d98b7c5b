//includes ---------------------------------------------------
#include "clock_config.h"
#include "MKE02Z4.h"

#include "fsl_adc.h"
#include "fsl_common.h"
#include "fsl_gpio.h"
#include "fsl_port.h"
#include "fsl_uart.h"

//variables ---------------------------------------------------
volatile uint32_t msTicks = 0;

//functions ---------------------------------------------------
void SysTick_Handler() {
    msTicks += 1;
}

void delay(uint32_t ms) {
    uint32_t returnTime = msTicks + ms;
    while (msTicks < returnTime) {}
}

void gpio_Init(void)
{

	gpio_pin_config_t port_config = { kGPIO_DigitalOutput, 1};

}

//main --------------------------------------------------------
int main(void)
{
	SysTick_Handler();

	while(1)
	{


	}

}



